{
  "name": "@sbpmjs/modeler",
  "version": "0.1.0",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "vite ./dev",
    "build": "vite build && tsc",
    "test": "vite ./test",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --list-different .",
    "format:fix": "prettier --write .",
    "prepare": "vite build && tsc",
    "size": "size-limit"
  },
  "author": "Maksym Kolodiy",
  "size-limit": [
    {
      "path": "dist/modeler.cjs.production.min.js",
      "limit": "200 KB"
    },
    {
      "path": "dist/modeler.esm.js",
      "limit": "200 KB"
    }
  ],
  "dependencies": {
    "jointjs": "^3.5.1"
  }
}
