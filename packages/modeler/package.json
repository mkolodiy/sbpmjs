{
  "name": "@sbpmjs/modeler",
  "version": "1.0.2",
  "description": "A library for working with SBPM constructs.",
  "keywords": [
    "sbpm",
    "modeler"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "vite ./dev",
    "build": "vite build && tsc",
    "test": "vite ./test",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "format": "prettier --list-different src",
    "format:fix": "prettier --write src",
    "prepare": "vite build && tsc",
    "size": "size-limit"
  },
  "author": "Maksym Kolodiy",
  "size-limit": [
    {
      "path": "./dist/index.js",
      "limit": "200 KB"
    }
  ],
  "dependencies": {
    "jointjs": "^3.5.1"
  }
}
