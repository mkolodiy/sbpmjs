{
  "name": "@sbpmjs/modeler",
  "version": "1.0.0-alpha.2",
  "description": "A library for modeling SBPM processes.",
  "keywords": [
    "sbpm",
    "modeler"
  ],
  "author": "Maksym Kolodiy <mak.kolodiy@gmail.com>",
  "homepage": "https://github.com/mkolodiy/sbpmjs/tree/master/packages/modeler#readme",
  "license": "MIT",
  "main": "dist/modeler.umd.js",
  "module": "dist/modeler.es5.js",
  "typings": "dist/types/modeler.d.ts",
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkolodiy/sbpmjs.git"
  },
  "scripts": {
    "clean": "rimraf dist docs coverage",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"lib/**/*.ts\" --write",
    "fix:tslint": "tslint --fix --project .",
    "lint": "tslint  --project tsconfig.json -t codeFrame 'lib/**/*.ts' '__test__/**/*.ts' && prettier \"lib/**/*.ts\" --list-different",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "run-s lint test -- --no-cache",
    "prebuild": "run-s clean fix",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && typedoc --out docs --target es6 --theme minimal --mode file lib"
  },
  "bugs": {
    "url": "https://github.com/mkolodiy/sbpmjs/issues"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "lib/*.{js,ts}"
    ]
  },
  "dependencies": {
    "jointjs": "^3.1.1"
  },
  "devDependencies": {}
}
