{
  "name": "@sbpmjs/modeler",
  "version": "1.0.2",
  "description": "A library for working with SBPM constructs.",
  "keywords": [
    "sbpm",
    "modeler"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dev": "vite --config vite.config.ts ./dev",
    "build": "vite build && tsc",
    "test": "vite --config vite.config.ts ./test",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --list-different .",
    "format:fix": "prettier --write ."
  },
  "author": "Maksym Kolodiy",
  "dependencies": {
    "@sbpmjs/shared": "workspace:^",
    "jointjs": "^3.7.6"
  }
}
